<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 원형 도형 -->
    <item>
        <shape android:shape="oval">
            <stroke
                android:width="4px"
                android:color="#009471" />
            <solid
                android:color="#FFFAED" />
            <size
                android:width="10dp"
                android:height="10dp" />
        </shape>
    </item>

    <!-- 이미지 -->
    <item android:drawable="@drawable/ic_chat_profile"
        android:start="7dp"
        android:top="5dp">
        <!-- 이미지의 위치 및 크기 조절 -->
    </item>
</layer-list>